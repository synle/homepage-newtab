{
  "name": "homepage-newtab",
  "version": "0.0.1",
  "manifest_version": 3,
  "description": "This extension will let you set a homepage for new tab.",
  "homepage_url": "https://synle.github.io/homepage-newtab/",
  "icons": {
    "16": "icons/icon.png",
    "48": "icons/icon.png",
    "128": "icons/icon.png"
  },
  "options_page": "src/options/index.html",
  "chrome_url_overrides": {
    "newtab": "src/override/override.html"
  }
}
